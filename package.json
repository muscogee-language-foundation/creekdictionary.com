{
  "name": "reason-react-ssr-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"npm run js-watch\" \"npm run bsb-watch\"",
    "build": "npm run bsb-clean && npm run bsb-build && npm run js-build",
    "serve": "node src/server.js",
    "js-watch": "webpack-dev-server --hot --inline --config webpack/webpack.config.dev.js",
    "js-build": "webpack --config webpack/webpack.config.prod.js",
    "bsb-watch": "bsb -make-world -w",
    "bsb-build": "bsb -make-world",
    "bsb-clean": "bsb -clean-world"
  },
  "dependencies": {
    "bs-css": "8.0.4",
    "emotion": "10.0.9",
    "emotion-server": "10.0.9",
    "express": "4.17.1",
    "normalize.css": "8.0.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "reason-react": "0.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.6",
    "bs-platform": "5.0.4",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "4.1.0",
    "css-loader": "3.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.7.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.1.3",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.4",
    "webpack-dev-server": "3.7.2",
    "webpack-manifest-plugin": "2.0.4"
  }
}
