    "normalize.css": "8.0.1",
    "prerender-node": "3.2.5",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "reason-loadable": "0.6.2",
    "reason-react": "0.7.0",
    "reason-react-update": "1.0.0",
    "tailwindcss": "1.1.4",
    "webpack-stats-plugin": "0.3.1"
  },
