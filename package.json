    "normalize.css": "8.0.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "reason-loadable": "0.6.2",
    "reason-react": "0.7.0",
    "relude": "0.28.1",
    "relude-fetch": "0.6.0",
    "relude-reason-react": "0.7.0",
    "tailwindcss": "1.1.2",
    "webpack-stats-plugin": "0.3.0"
